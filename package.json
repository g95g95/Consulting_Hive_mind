{
  "name": "consulting-hive-mind",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint",
    "setup": "node scripts/setup.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:seed": "npx tsx prisma/seed-e2e.ts",
    "test:e2e:clean": "npx tsx -e \"const{PrismaClient}=require('@prisma/client');const p=new PrismaClient();(async()=>{const ids=['e2e_client_user','e2e_consultant_user','e2e_both_user','e2e_new_user'];const users=await p.user.findMany({where:{clerkId:{in:ids}},select:{id:true}});if(users.length){await p.user.deleteMany({where:{id:{in:users.map(u=>u.id)}})}await p.$disconnect();console.log('E2E data cleaned')})()\"",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:create": "prisma migrate dev --create-only",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:setup": "prisma generate && prisma db push"
  },
